<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>@ViewBag.TitleBreadcumb</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/">Inicio</a>
            </li>
            @if (ViewBag.Controller != null) {
                <li class="breadcrumb-item">
                    @ViewBag.Controller
                </li>
            }
            <li class="active breadcrumb-item">
                <strong>@ViewBag.Active</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">
    </div>
</div>